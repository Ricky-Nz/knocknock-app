    "react-dom": "^0.14.8",
    "react-dropzone": "^3.4.0",
    "react-redux": "^4.4.1",
    "react-router-redux": "^4.0.1",
    "react-tap-event-plugin": "^0.2.2",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",

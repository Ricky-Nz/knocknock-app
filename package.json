{
  "name": "knocknockapp",
  "version": "1.0.0",
  "description": "Knocknock mobile web application",
  "scripts": {
    "start": "babel-node ./server/index.js",
    "update": "babel-node ./graphql-update-schema.js",
    "dev": "webpack-dev-server --config webpack-dev.config.js --content-base dist/ --progress --inline --colors --hot",
    "build": "NODE_ENV=production webpack -p --config webpack-production.config.js",
    "test": "mocha --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch"
  },
  "keywords": [
    "React"
  ],
  "author": "Ruiqi",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.7.4",
